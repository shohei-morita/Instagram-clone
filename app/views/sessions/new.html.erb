<h1>ログインページ</h1>

<%= form_with(scope: :session, url: sessions_path, local: true) do |form| %>
<div>
  <%= form.label :email %>
  <%= form.email_field :email %>
</div>
<div>
  <%= form.label :password %>
  <%= form.password_field :password %>
</div>
<div>
  <%= form.submit %q(Log in) %>
</div>
<% end %>
